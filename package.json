{
  "name": "ormacarbon",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:mkvlrn/ormacarbon-backend-test",
  "author": "Mike Valeriano <mkvlrn@gmail.com>",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "prebuild": "yarn rimraf build",
    "build": "yarn tsc -p tsconfig.build.json && yarn tsc-alias -p tsconfig.build.json",
    "dev": "yarn tsx --watch src/index.ts",
    "start": "node build",
    "lint": "yarn eslint --fix",
    "format": "yarn prettier --write",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules yarn jest",
    "check-types": "yarn tsc-files",
    "test-staged": "cross-env NODE_OPTIONS=--experimental-vm-modules yarn jest --bail --findRelatedTests",
    "prepare": "yarn husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@golevelup/ts-jest": "^0.3.4",
    "@jest/globals": "^29.4.2",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.4.2",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^13.1.1",
    "prettier": "^2.8.4",
    "prisma": "^4.10.1",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-files": "^1.1.3",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@prisma/client": "4.10.1",
    "auto-bind": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0"
  }
}
