{
  "name": "orma-carbon",
  "version": "0.1.0",
  "description": "Typescript+Express+Jest boilerplate",
  "author": "ZauJulio<zaujulio.dev@gmail.com>",
  "repository": "git@github.com:ZauJulio/backend-test-two.git",
  "main": "src/main.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "start": "node dist/main.js",
    "dev": "DEBUG=* tsx watch src/main.ts",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**/*.ts\" --fix .",
    "format": "prettier \"src/**/*.ts\" -u -w",
    "scan": "./scripts/sonar-scanner.sh",
    "prepare": "husky install"
  },
  "tsup": {
    "entry": [
      "src/main.ts"
    ],
    "splitting": false,
    "clean": true
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-winston": "^4.2.0",
    "helmet": "^6.0.1",
    "swagger-ui-express": "^4.6.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.1",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "git-commit-msg-linter": "^4.5.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4"
  }
}
