{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "ES2019",                                
    "module": "commonjs",                              
    "forceConsistentCasingInFileNames": true,          
    "strict": true,                                    
    "skipLibCheck": true,
    "strictNullChecks": false,
    "esModuleInterop": true
  },
  "include": ["src"],
  "exclude": [
    "prisma"
  ]
}
