{
  "compilerOptions": {
    "target": "es2019",                                 
    "module": "commonjs",                               
    "rootDir": "src",     
    "outDir": "./dist",                             
    "allowJs": true,                                 
    "esModuleInterop": true,                            
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                      
    "skipLibCheck": true,
    "baseUrl": "src",                
  },
  "include": [
    "src"
  ],
  "exclude": [] 
}
